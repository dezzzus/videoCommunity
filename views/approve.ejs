<% include head_begin.ejs %>
<% include head_end.ejs %>
<div class="container">
    <table class="table table-hover">
        <thead>
        <tr>
            <th>Email</th>
            <th></th>
        </tr>
        </thead>
        <tbody>
        <% for (var i = 0; i < agents.length;i++){ %>
        <tr>
            <td><%= agents[i].email %></td>
            <td>
                <% if(!agents[i].approved){ %>
                <a href="/approve/<%= agents[i]._id %>" class="btn btn-success">Approve</a>
                <% }else{ %>
                Approved
                <% } %>
            </td>
        </tr>
        <% } %>
        </tbody>
    </table>
</div>
<% include foot.ejs %>